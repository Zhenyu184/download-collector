{
    "manifest_version": 3,
    "name": "Download Collector",
    "version": "0.1",
    "description": "An extension to listen to download events",
    "permissions": ["downloads", "downloads.shelf"],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": "images/icon16.png"
    }
}
